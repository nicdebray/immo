    <h1>Rendements locatifs</h1>
    <h2>Calculez facilement le rendement locatif de vos biens immobiliers</h2>
    <p> <%= link_to 'New Project', new_project_path, class: 'btn btn-primary btn-homepage' %> </p>
    <p> <%= link_to 'All My Projects', projects_path, class: 'btn btn-primary btn-homepage' %> </p>

    <!-- estimator block start  -->
    <div id="homepage-estimator">
      <div id="toggle-estimator">Quick estimate:</div>
      <div id="estimator-content">
        <div id="purchase-price-input" class="estimator-fields">
          <label for="purchase-price-input-estimator">Purchase price:</label>
          <input type="number" min="0" value="0" id="purchase-price-input-estimator" class="home-input-estimator">
        </div>

        <div id="extra-work-input" class="estimator-fields">
          <label for="extra-works-input-estimator">Extra works:</label>
          <input type="number" min="0" value="0" id="extra-works-input-estimator" class="home-input-estimator">
        </div>

        <div id="monthly-rent-input" class="estimator-fields">
          <label for="monthly-rent-input-estimator">Monthly rent:</label>
          <input type="number" min="0" value="0" id="monthly-rent-input-estimator" class="home-input-estimator">
        </div>

        <div id="result-estimator" class="estimator-fields">
          <div id="result-estimator10"></div>
          <div id="result-estimator11"></div>
          <div id="result-estimator12"></div>
        </div>
      </div>
      <div id="estimator-checker">
        <button class="btn btn-success" id="estimator-checker-btn"> Estimate</button>
      </div>
    </div>
    <!-- estimator block end -->
<%= javascript_pack_tag 'estimator_homepage' %>
