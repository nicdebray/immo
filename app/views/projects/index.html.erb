<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>
<p> <%= link_to 'new', new_project_path, class: 'btn btn-primary' %> </p>
<% @projects.each do |project| %>
  <div class="card">
    <%= project.name %>
    <%= project.address %>
    <i>
    <%= link_to "", project_path(project), method: :delete, data: {confirm: "Are you sure?"}, class: "far fa-trash-alt" %>
    </i>
    <i>
    <%= link_to "", edit_project_path(project), class: "far fa-edit" %>
    </i>
  </div>
<% end %>
